PImage elvis;
PImage grid;

void setup () {
  size (800, 150, P2D);
  elvis = loadImage ("elvis.png");
  grid = loadImage ("grid.png");
}

void draw () {
  background (88, 117, 121);
  image (elvis, 15, 4);
  image (grid, 150, 15);
}

